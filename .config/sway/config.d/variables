set $mod Mod4 
set $left h
set $down j
set $up k
set $right l

set $browser google-chrome-stable --enable-features=UseOzonePlatform --ozone-platform=wayland
set $filemanager thunar
set $gnome_schema org.gnome.desktop.interface
set $menu wofi --show run 
set $screenshot_path $HOME/.config/sway/scripts/screenshot.sh
set $term alacritty
set $texteditor code
set $wallpaper $HOME/wallpaper.png

set $color1 #CDDEDE
set $color2 #171D1D
set $background_color #434c5e
set $foreground_color #eceff4
